{
  "name": "pocre-api",
  "version": "1.0.0",
  "description": "POCRE (Proof Of co-CREation) Rest API",
  "main": "src/index.ts",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "build": "cross-env NODE_ENV=production babel src --config-file ./babel.config.js --out-dir ./dist --extensions .ts --out-file-extension .js --copy-files",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prettier": "prettier --check ./src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts",
    "docker:up": "docker-compose -f docker-compose.yml up",
    "docker:down": "docker-compose -f docker-compose.yml down",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@sendgrid/mail": "^7.7.0",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.7",
    "@types/pg": "^8.6.5",
    "@types/qrcode": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.3.0",
    "file-type": "^16.5.4",
    "got": "^11.8.6",
    "helmet": "^4.1.0",
    "http-status": "^1.4.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "openapi-types": "^9.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "pm2": "^5.1.0",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.0.8",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.3.9",
    "validator": "^13.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^12.0.0",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-jest": "^27.0.6",
    "coveralls": "^3.0.7",
    "cross-env": "^7.0.3",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.0.5",
    "ts-node": "^10.1.0",
    "typescript": "^4.1.2"
  }
}
